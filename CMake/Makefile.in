#!gmake
# MAGIC_IS_BUILDYARD_MAKEFILE

all:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-buildonly

install:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-install

deps:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-deps

package:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-package
	
package-upload:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-package-upload

configure:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-Makefile

test:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-testonly

clean:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-clean

doxygen:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-doxygen

doxygen-upload:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-doxygen-upload

github:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ @NAME@-github

.DEFAULT:
	@$(MAKE) -C @CMAKE_SOURCE_DIR@ $(MAKECMDGOALS)
